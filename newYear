import React, {Component} from 'react';

class NewYear2019 extends Component {
	render() {
		const newYear2019data = new Date(2019, 0, 1, 0, 0, 0, 0);
		
			if (Date.now() >= newYear2019data) {
				// Тут валиш картінку
			}  else {
				return <h1>New Year coming soon!</h1>	
			}
	}
}

export default NewYear2019;
